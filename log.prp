log4j.appender.RMRFA.rollingPolicy.FileNamePattern=${hadoop.log.dir}/${hadoop.log.file}-%d{yyyyMMdd-HH}.log.gz
log4j.appender.RMRFA.layout=org.apache.log4j.PatternLayout
