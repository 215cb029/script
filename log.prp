log4j.appender.RMRFA.triggeringPolicy=org.apache.log4j.rolling.SizeBasedTriggeringPolicy
log4j.appender.RMRFA.triggeringPolicy.maxFileSize=268435456
#
# HDFS block state change log from block manager
#
# Uncomment the following to suppress normal block state change
# messages from BlockManager in NameNode.
#log4j.logger.BlockStateChange=WARN
#Relative to Yarn Log Dir Prefix
yarn.log.dir=.
#
# Job Summary Appender
#
# Use following logger to send summary to separate file defined by
# hadoop.mapreduce.jobsummary.log.file rolled daily:
# hadoop.mapreduce.jobsummary.logger=INFO,JSA
#
# Set the ResourceManager summary log filename
yarn.server.resourcemanager.appsummary.log.file=hadoop-mapreduce.jobsummary.log
# Set the ResourceManager summary log level and appender
yarn.server.resourcemanager.appsummary.logger=${hadoop.root.logger}
#yarn.server.resourcemanager.appsummary.logger=INFO,RMSUMMARY

# To enable AppSummaryLogging for the RM,
# set yarn.server.resourcemanager.appsummary.logger to
# LEVEL,RMSUMMARY in hadoop-env.sh

# Appender for ResourceManager Application Summary Log
# Requires the following properties to be set
#    - hadoop.log.dir (Hadoop Log directory)
#    - yarn.server.resourcemanager.appsummary.log.file (resource manager app summary log filename)
#    - yarn.server.resourcemanager.appsummary.logger (resource manager app summary log level and appender)
log4j.appender.RMSUMMARY=org.apache.log4j.rolling.RollingFileAppender
log4j.appender.RMSUMMARY.rollingPolicy=org.apache.log4j.rolling.FixedWindowRollingPolicy
log4j.appender.RMSUMMARY.rollingPolicy.maxIndex=150
log4j.appender.RMSUMMARY.rollingPolicy.ActiveFileName=${yarn.log.dir}/${yarn.server.resourcemanager.appsummary.log.file}
log4j.appender.RMSUMMARY.rollingPolicy.FileNamePattern=${yarn.log.dir}/${yarn.server.resourcemanager.appsummary.log.file}-%i.log.gz
log4j.appender.RMSUMMARY.layout=org.apache.log4j.PatternLayout
log4j.appender.RMSUMMARY.layout.ConversionPattern=%d{ISO8601} %p %c{2}: %m%n
log4j.appender.RMSUMMARY.triggeringPolicy=org.apache.log4j.rolling.SizeBasedTriggeringPolicy
log4j.appender.RMSUMMARY.triggeringPolicy.MaxFileSize=268435456

log4j.appender.JSA=org.apache.log4j.DailyRollingFileAppender
log4j.appender.JSA.rollingPolicy=org.apache.log4j.rolling.FixedWindowRollingPolicy
log4j.appender.JSA.rollingPolicy.maxIndex=12
log4j.appender.JSA.rollingPolicy.ActiveFileName=${yarn.log.dir}/${yarn.server.resourcemanager.appsummary.log.file}
log4j.appender.JSA.rollingPolicy.FileNamePattern=${yarn.log.dir}/${yarn.server.resourcemanager.appsummary.log.file}-%i.log.gz
log4j.appender.JSA.layout=org.apache.log4j.PatternLayout
log4j.appender.JSA.layout.ConversionPattern=%d{yy/MM/dd HH:mm:ss} %p %c{2}: %m%n
log4j.appender.JSA.triggeringPolicy=org.apache.log4j.rolling.SizeBasedTriggeringPolicy
log4j.appender.JSA.triggeringPolicy.MaxFileSize=268435456

log4j.appender.FILETimelineWriter=org.apache.log4j.RollingFileAppender
log4j.appender.FILETimelineWriter.DatePattern='.'yyyy-MM-dd
log4j.appender.FILETimelineWriter.File=${greensight.dir}/${greensight.timeline.file}
log4j.appender.FILETimelineWriter.Append=true
log4j.appender.FILETimelineWriter.layout=org.apache.log4j.PatternLayout
log4j.appender.FILETimelineWriter.level=INFO
log4j.appender.FILETimelineWriter.threshold=INFO
log4j.appender.FILETimelineWriter.maxFileSize=9GB
log4j.appender.FILETimelineWriter.MaxBackupIndex=20
log4j.appender.FILETimelineWriter.layout.ConversionPattern=%d{ISO8601} - %m%n

log4j.logger.com.flipkart.fdp.greensight.collector.timeline.v1.writer.LoggerTimelineWriter=${greensight.timeline.logger}
log4j.additivity.com.flipkart.fdp.greensight.collector.timeline.v1.writer.LoggerTimelineWriter=false
