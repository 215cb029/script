log4j.appender.NNRFA.rollingPolicy.ActiveFileName=${hadoop.log.dir}/${hadoop.log.file}
log4j.appender.NNRFA.rollingPolicy.FileNamePattern=${hadoop.log.dir}/${hadoop.log.file}-%d{yyyyMMdd-HH}.log.gz
log4j.appender.NNRFA.layout=org.apache.log4j.PatternLayout
log4j.appender.NNRFA.layout.ConversionPattern=%d{ISO8601} %-5p %c{2} (%F:%M(%L)) - %m%n
