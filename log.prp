# Define some default values that can be overridden by system properties
# To change daemon root logger use hadoop_root_logger in hadoop-env

#Default values
hadoop.root.logger=WARN,console
hadoop.log.dir=.
hadoop.log.file=hadoop.log
hadoop.security.log.maxbackupindex=20
hadoop.tasklog.taskid=null
hadoop.tasklog.iscleanup=false
hadoop.tasklog.noKeepSplits=4
hadoop.tasklog.totalLogFileSize=100
hadoop.tasklog.purgeLogSplits=true
hadoop.tasklog.logsRetainHours=12
hadoop.mapreduce.jobsummary.logger=${hadoop.root.logger}
hadoop.mapreduce.jobsummary.log.file=hadoop-mapreduce.jobsummary.log
log4j.category.SecurityLogger=${hadoop.security.logger}
hadoop.security.log.file=SecurityAuth.audit
hadoop.metrics.log.level=INFO
mapred.audit.logger=INFO,console
hdfs.audit.logger=INFO,console
hadoop.security.logger=INFO,console
log4j.threshhold=ALL
greensight.dir=${hadoop.log.dir}
greensight.timeline.file=greensight-timeline-writer.out
greensight.timeline.logger=INFO, FILETimelineWriter

#Changing log level for ranger class
log4j.logger.org.apache.ranger.plugin.policyengine.RangerPolicyEngineImpl=WARN, NNRFA


#Turning off logs for provider.BaseAuditHandler class
log4j.logger.org.apache.ranger.audit.provider.BaseAuditHandler=ALL, exceptionAppender
log4j.additivity.org.apache.ranger.audit.provider.BaseAuditHandler=false
log4j.appender.exceptionAppender=org.apache.log4j.DailyRollingFileAppender
log4j.appender.exceptionAppender.File=${hadoop.log.dir}/SecurityAuth.audit
log4j.appender.exceptionAppender.datePattern='-'dd'.log'
log4j.appender.exceptionAppender.layout=org.apache.log4j.PatternLayout
log4j.appender.exceptionAppender.layout.ConversionPattern=%-6r %d{ISO8601} %-5p %40.40c %x - %m\n




log4j.additivity.org.apache.hadoop.hdfs.server.namenode.FSNamesystem.audit=false
log4j.additivity.org.apache.hadoop.mapred.AuditLogger=false
log4j.logger.org.apache.hadoop.mapred.AuditLogger=${mapred.audit.logger}
log4j.logger.org.apache.hadoop.metrics2=${hadoop.metrics.log.level}
log4j.logger.org.jets3t.service.impl.rest.httpclient.RestS3Service=ERROR
log4j.logger.org.apache.hadoop.conf.Configuration.deprecation=WARN
log4j.logger.org.apache.hadoop.hdfs.server.namenode.FSNamesystem.audit=${hdfs.audit.logger}
log4j.rootLogger=${hadoop.root.logger}, EventCounter

log4j.appender.NullAppender=org.apache.log4j.varia.NullAppender
log4j.appender.EventCounter=org.apache.hadoop.log.metrics.EventCounter
log4j.logger.org.apache.hadoop.yarn.server.resourcemanager.RMAppManager$ApplicationSummary=${yarn.server.resourcemanager.appsummary.logger}
log4j.additivity.org.apache.hadoop.yarn.server.resourcemanager.RMAppManager$ApplicationSummary=false

# Define the root logger to the system property "hadoop.root.logger".


log4j.appender.console=org.apache.log4j.ConsoleAppender
log4j.appender.console.target=System.err
log4j.appender.console.layout=org.apache.log4j.PatternLayout
log4j.appender.console.layout.ConversionPattern=%d{yy/MM/dd HH:mm:ss} %p %c{2}: %m%n

log4j.appender.TLA=org.apache.hadoop.mapred.TaskLogAppender
log4j.appender.TLA.taskId=${hadoop.tasklog.taskid}
log4j.appender.TLA.isCleanup=${hadoop.tasklog.iscleanup}
log4j.appender.TLA.totalLogFileSize=${hadoop.tasklog.totalLogFileSize}
log4j.appender.TLA.layout=org.apache.log4j.PatternLayout
log4j.appender.TLA.layout.ConversionPattern=%d{ISO8601} %p %c: %m%n

log4j.appender.DRFA=org.apache.log4j.DailyRollingFileAppender
log4j.appender.DRFA.File=${hadoop.log.dir}/${hadoop.log.file}
log4j.appender.DRFA.DatePattern=.yyyy-MM-dd-HH
log4j.appender.DRFA.layout=org.apache.log4j.PatternLayout
log4j.appender.DRFA.layout.ConversionPattern=%d{ISO8601} %p %c: %m%n

log4j.appender.DRFAS=org.apache.log4j.DailyRollingFileAppender
log4j.appender.DRFAS.rollingPolicy=org.apache.log4j.rolling.FixedWindowRollingPolicy
log4j.appender.DRFAS.rollingPolicy.maxIndex=12
log4j.appender.DRFAS.rollingPolicy.ActiveFileName=${hadoop.log.dir}/${hadoop.security.log.file}.log
log4j.appender.DRFAS.rollingPolicy.FileNamePattern=${hadoop.log.dir}/${hadoop.security.log.file}-%i.log.gz
log4j.appender.DRFAS.layout=org.apache.log4j.PatternLayout
log4j.appender.DRFAS.layout.ConversionPattern=%d{ISO8601} %p %c: %m%n
log4j.appender.DRFAS.triggeringPolicy=org.apache.log4j.rolling.SizeBasedTriggeringPolicy
log4j.appender.DRFAS.triggeringPolicy.MaxFileSize=268435456

log4j.appender.RFAS=org.apache.log4j.rolling.RollingFileAppender
log4j.appender.RFAS.rollingPolicy=org.apache.log4j.rolling.FixedWindowRollingPolicy
log4j.appender.RFAS.rollingPolicy.maxIndex=12
log4j.appender.RFAS.rollingPolicy.ActiveFileName=${hadoop.log.dir}/${hadoop.security.log.file}.log
log4j.appender.RFAS.rollingPolicy.FileNamePattern=${hadoop.log.dir}/${hadoop.security.log.file}-%i.log.gz
log4j.appender.RFAS.layout=org.apache.log4j.PatternLayout
log4j.appender.RFAS.layout.ConversionPattern=%d{ISO8601} %p %c: %m%n
log4j.appender.RFAS.triggeringPolicy=org.apache.log4j.rolling.SizeBasedTriggeringPolicy
log4j.appender.RFAS.triggeringPolicy.MaxFileSize=268435456

log4j.appender.RFAAUDIT=org.apache.log4j.rolling.RollingFileAppender
log4j.appender.RFAAUDIT.rollingPolicy=org.apache.log4j.rolling.TimeBasedRollingPolicy
log4j.appender.RFAAUDIT.rollingPolicy.ActiveFileName=${hadoop.log.dir}/hdfs-audit.log
log4j.appender.RFAAUDIT.rollingPolicy.FileNamePattern=${hadoop.log.dir}/hdfs-audit-%d{yyyyMMdd-HH}.log.gz
log4j.appender.RFAAUDIT.layout=org.apache.log4j.PatternLayout
log4j.appender.RFAAUDIT.layout.ConversionPattern=%d{ISO8601} %p %c{2}: %m%n
log4j.appender.RFAAUDIT.triggeringPolicy=org.apache.log4j.rolling.SizeBasedTriggeringPolicy
log4j.appender.RFAAUDIT.triggeringPolicy.maxFileSize=1073741824

log4j.appender.DRFAAUDIT=org.apache.log4j.rolling.RollingFileAppender
log4j.appender.DRFAAUDIT.rollingPolicy=org.apache.log4j.rolling.FixedWindowRollingPolicy
log4j.appender.DRFAAUDIT.rollingPolicy.maxIndex=300
log4j.appender.DRFAAUDIT.rollingPolicy.ActiveFileName=${hadoop.log.dir}/hdfs-audit.log
log4j.appender.DRFAAUDIT.rollingPolicy.FileNamePattern=${hadoop.log.dir}/hdfs-audit-%i.log.gz
log4j.appender.DRFAAUDIT.layout=org.apache.log4j.PatternLayout
log4j.appender.DRFAAUDIT.layout.ConversionPattern=%d{ISO8601} %p %c{2}: %m%n
log4j.appender.DRFAAUDIT.triggeringPolicy=org.apache.log4j.rolling.SizeBasedTriggeringPolicy
log4j.appender.DRFAAUDIT.triggeringPolicy.MaxFileSize=1073741824


log4j.appender.MRAUDIT=org.apache.log4j.rolling.RollingFileAppender
log4j.appender.MRAUDIT.rollingPolicy=org.apache.log4j.rolling.FixedWindowRollingPolicy
log4j.appender.MRAUDIT.rollingPolicy.maxIndex=12
log4j.appender.MRAUDIT.rollingPolicy.ActiveFileName=${hadoop.log.dir}/mapred-audit.log
log4j.appender.MRAUDIT.rollingPolicy.FileNamePattern=${hadoop.log.dir}/mapred-audit-%i.log.gz
log4j.appender.MRAUDIT.layout=org.apache.log4j.PatternLayout
